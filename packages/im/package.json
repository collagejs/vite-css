{
  "name": "@collagejs/vite-im",
  "version": "0.1.0",
  "description": "Vite plug-in that injects an import map and import-map-overrides to the project's HTML page",
  "keywords": [
    "importmap",
    "vite",
    "vite-plugin",
    "micro-frontend",
    "collagejs",
    "mfe"
  ],
  "homepage": "https://collagejs.dev",
  "bugs": {
    "url": "https://github.com/collagejs/vite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collagejs/vite.git"
  },
  "license": "MIT",
  "author": {
    "name": "José Pablo Ramírez Vargas",
    "email": "webJose@gmail.com"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ts-mocha -n loader=ts-node/esm -p ./tsconfig.json ./tests/**/*.test.ts",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc && publint",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "chai": "^6.2.1",
    "publint": "^0.3.16",
    "rimraf": "^6.1.2",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
